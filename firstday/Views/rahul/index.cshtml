
@model List < firstday.Models.employee>


@{
    ViewBag.Title = "index";
    int sn= 0;
}
<link rel="stylesheet" href="~/Content/index.css"/>
<div id="form">
<h2>Employees</h2>
<table border="1">
    <tr>
        <th>SN</th>
        <th>ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Contact</th>
    </tr>
        @foreach(var data in Model)
            {
                <tr>
                    <td>@(++sn)</td>
                    <td>@data.id</td>
                    <td>@data.name</td>
                    <td>@data.address</td>
                    <td>@data.contact</td>
                </tr>
            }
    
    
</table>
</div>
<a href="@Url.Action("create")"><h4>Add new records</h4></a>

<script src="~/Scripts/index.js"></script>

